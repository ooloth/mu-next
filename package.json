    "mdx-prism": "^0.3.1",
    "next": "10.2.0",
    "next-mdx-remote": "^3.0.1",
    "next-sanity": "^0.1.12",
    "next-seo": "^4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.3.1",
    "react-tweet-embed": "^1.2.2",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.2",
    "remark-slug": "^6.0.0",
    "rss": "^1.2.2",
